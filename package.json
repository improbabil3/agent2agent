    "start:multi-system": "node src/basic-agents/multi-agent-system.js",
    "start:multi-client": "node src/basic-agents/multi-agent-client.js",
    "start:agent1": "node src/basic-agents/agent1-server.js",
    "start:agent2": "node src/basic-agents/agent2-server.js",
    "start:agent3": "node src/basic-agents/agent3-server.js",
    "start:simple-text": "node src/simple-text-agent.js",
    "start:simple-sentiment": "node src/simple-sentiment-agent.js",
